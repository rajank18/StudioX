    {
      "name": "Tasks",
      "item": [
        {
          "name": "Create Task",
          "request": {
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{clerk_token}}",
                  "type": "string"
                }
              ]
            },
            "method": "POST",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"type\": \"image_analysis\",\n  \"input\": \"https://example.com/image.jpg\",\n  \"metadata\": {\n    \"analysisType\": \"object_detection\"\n  }\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "http://localhost:3000/api/tasks/create",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3000",
              "path": ["api", "tasks", "create"]
            }
          }
        },
        {
          "name": "Get All User Tasks",
          "request": {
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{clerk_token}}",
                  "type": "string"
                }
              ]
            },
            "method": "GET",
            "url": {
              "raw": "http://localhost:3000/api/tasks",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3000",
              "path": ["api", "tasks"]
            }
          }
        },
        {
          "name": "Get Task by ID",
          "request": {
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{clerk_token}}",
                  "type": "string"
                }
              ]
            },
            "method": "GET",
            "url": {
              "raw": "http://localhost:3000/api/tasks/{{task_id}}",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3000",
              "path": ["api", "tasks", "{{task_id}}"]
            }
          }
        },
        {
          "name": "Get Task Stats",
          "request": {
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{clerk_token}}",
                  "type": "string"
                }
              ]
            },
            "method": "GET",
            "url": {
              "raw": "http://localhost:3000/api/tasks/stats",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3000",
              "path": ["api", "tasks", "stats"]
            }
          }
        },
        {
          "name": "Cancel Task",
          "request": {
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{clerk_token}}",
                  "type": "string"
                }
              ]
            },
            "method": "PATCH",
            "url": {
              "raw": "http://localhost:3000/api/tasks/{{task_id}}/cancel",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3000",
              "path": ["api", "tasks", "{{task_id}}", "cancel"]
            }
          }
        },
        {
          "name": "Update Task Status",
          "request": {
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{clerk_token}}",
                  "type": "string"
                }
              ]
            },
            "method": "PATCH",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"status\": \"completed\",\n  \"output\": \"Result URL or data\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "http://localhost:3000/api/tasks/{{task_id}}/status",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3000",
              "path": ["api", "tasks", "{{task_id}}", "status"]
            }
          }
        }
